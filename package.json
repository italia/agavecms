{
  "name": "agave-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "lint:autocorrect": "eslint src --fix",
    "build:clean": "rm -f public/assets/*.js public/assets/*.map public/assets/*.css || :",
    "build:webfonts": "mkdir -p public/assets/fonts && bemo-webfonts -s assets/fonts/svg -f public/assets/fonts -S styles/variables/_icon-glyphs.scss",
    "build:js": "NODE_ENV=production webpack --progress",
    "build": "yarn run build:clean && yarn run build:webfonts && yarn run build:js",
    "dev": "NODE_ENV=development APP_VERSION=development webpack-dev-server --mode development --public --host 0.0.0.0 --progress --inline --hot",
    "build:dev": "yarn run build:clean && yarn run build:webfonts && yarn run dev",
    "postinstall": "node -e \"try { require('fs').symlinkSync(require('path').resolve('node_modules/@bower_components'), 'bower_components', 'junction') } catch (e) { }\"",
    "cy:beforeSuite": "RAILS_ENV=test rails db:drop db:create db:migrate > /dev/null",
    "cy:ci": "yarn cy:beforeSuite && RAILS_ENV=test bundle exec rails server & yarn dev & yarn cy:run",
    "cy:dev": "yarn cy:beforeSuite && yarn cy:open",
    "cy:open": "cypress open",
    "cy:run": "cypress run"
  },
  "author": "Cantiere creativo Srl",
  "license": "MIT",
